{
  "extends": "./tsconfig.json",
  "exclude": [
    "test",
    "src/daos",
    "src/gh",
    "src/service",
    "src/snapshot",
    "scripts"
  ],
  "compilerOptions": {
    "noEmit": false,
    "outDir": "dist",
    "moduleResolution": "Node",
    "inlineSourceMap": true,
    "inlineSources": true,
    "declaration": true,
    "declarationMap": true,
    "declarationDir": "./dist/types",
    "removeComments": true,
    "preserveConstEnums": true
  }
}
